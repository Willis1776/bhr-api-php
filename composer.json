{
    "name": "bamboohr/api",
    "version": "2.1.0",
    "description": "Official PHP SDK for the BambooHR API. https://documentation.bamboohr.com",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://www.bamboohr.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "BambooHR",
            "homepage": "https://github.com/BambooHR/bhr-api-php"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "mustache/mustache": "^2.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"if (file_exists('vendor/bin/phpcs')) { passthru('./vendor/bin/phpcs --config-set installed_paths ' . getcwd() . '/vendor/bamboohr/phpcs'); }\""
        ],
        "post-update-cmd": [
            "@php -r \"if (file_exists('vendor/bin/phpcs')) { passthru('./vendor/bin/phpcs --config-set installed_paths ' . getcwd() . '/vendor/bamboohr/phpcs'); }\""
        ]
    },
    "autoload": {
        "psr-4": { "BhrSdk\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "BhrSdk\\Test\\" : "test/" }
    }
}
